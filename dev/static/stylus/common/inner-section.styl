.inner-section__wrap
  padding 50px 40px
  border 1px solid mercury

.inner-section__title
  font-size 34px
  font-weight 500
  line-height 36px
  letter-spacing 0.34px
  margin-bottom 45px

.inner-section__list
  display flex
  flex-wrap wrap

.inner-section__item
  display flex
  align-items center
  width 100%
  max-width 370px
  svg
    width 76px
    height 76px
    margin-right 65px
    flex none