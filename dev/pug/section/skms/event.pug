section.skms-event.inner-section(id=skms.event.id)
  .container
    .inner-section__wrap
      h3.inner-section__title.skms-event__title= skms.event.title
      each text in skms.event.texts
        p.text.inner-section__text= text
      .skms-event__list.inner-section__list
        each item in skms.event.list
          .skms-event__item.inner-section__item
            +icon(item["icon"])
            span.text= item.text